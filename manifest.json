{
	"name":"Buffer Status",
	"version":"1.0",
	"description":"Yet another extension for Buffer app.",
  "manifest_version": 2,
	"browser_action":{
		"default_icon":"images/icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
    	"tabs",
    	"debugger",
    	"https://bufferapp.com/oauth2/authorize",
      "https://api.bufferapp.com/1/oauth2/token.json",
      "https://api.bufferapp.com/"
  	]
}
